/**app.wxss**/
.container {
	height: 100%;
	display: flex;
	flex-direction: column;
	align-items: center;
	justify-content: space-between;
	padding: 200rpx 0;
	box-sizing: border-box;
}

.fl {
	float: left;
}

.fr {
	float: right;
}

.w-clear {
	clear: both;
}

.span-item {
	padding:0 20rpx;
	margin-top: 20rpx;
	/* height: 48rpx; */
}
.span-item span{
		color: #909090;
		font-size: 24rpx;
		background: #f8f8f8;
		padding: 10rpx 12rpx;
		margin-right: 12rpx;
}
.mar-top{
	margin-top: 25rpx;
}